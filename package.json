{
	"name": "express-react",
	"version": "1.0.0",
	"description": "express + react app",
	"main": "index.js",
	"scripts": {
		"dev-api": "cd ./api && yarn run dev",
		"dev-client": "cd ./client yarn start",
		"dev": "concurrently --kill-others \"yarn run dev-api\" \"yarn run dev-client\"",
		"start": "(cd ./api && yarn start) && (cd ../client && yarn start)",
		"postinstall": "(cd ./api && yarn install) && (cd ../client && yarn install)"
	},
	"keywords": [
		"express",
		"react"
	],
	"author": "David Nicholas",
	"license": "MIT",
	"dependencies": {
		"dot-env": "^0.0.1"
	},
	"devDependencies": {
		"concurrently": "^7.1.0",
		"wsrun": "^5.2.4"
	}
}
